{
  "name": "zwen",
  "description": "zwen is a code generator CLI for projects using react and redux",
  "version": "0.1.0",
  "author": "Zweitag GmbH @zweitag",
  "bin": {
    "zwen": "./bin/run"
  },
  "bugs": "https://github.com/https://github.com/zweitag/zwen-react/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/camelcase": "^4.1.0",
    "camelcase": "^5.0.0",
    "fs-extra": "^6.0.1",
    "tslib": "^1",
    "yeoman-environment": "^2.2.0",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/yeoman-generator": "^2.0.3",
    "ts-node": "^6",
    "typescript": "^2.9"
  },
  "engines": {
    "yarn": "Please use npm",
    "node": ">=8.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/https://github.com/zweitag/zwen-react",
  "keywords": [
    "oclif",
    "zweitag",
    "react",
    "redux",
    "generator",
    "cli",
    "architecture"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "zwen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/zweitag/zwen-react",
  "scripts": {
    "build": "rm -rf lib && tsc && cp -R src/generators/templates lib/generators/templates/",
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "test": "echo NO TESTS"
  },
  "types": "lib/index.d.ts"
}
