{
  "name": "zwen-react",
  "description": "Code generator CLI for projects using react and redux",
  "version": "0.6.0",
  "author": "Zweitag GmbH @zweitag",
  "bin": {
    "zwen": "./bin/run"
  },
  "bugs": "https://github.com/zweitag/zwen-react/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2.0.5",
    "camel-case": "^3.0.0",
    "constant-case": "^2.0.0",
    "ejs": "^2.6.1",
    "pascal-case": "^2.0.1",
    "yeoman-environment": "^2.2.0",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/ejs": "^2.6.0",
    "@types/jest-matchers": "^20.0.1",
    "@types/mem-fs": "^1.1.1",
    "@types/mem-fs-editor": "^4.0.1",
    "@types/node": "^10",
    "@types/yeoman-generator": "^2.0.3",
    "jest-matchers": "^20.0.3",
    "mocha": "^5.2.0",
    "ts-node": "^6",
    "tslib": "^1",
    "typescript": "^2.9"
  },
  "engines": {
    "yarn": "Please use npm",
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "plugins": [
    "@oclif/plugin-help"
  ],
  "homepage": "https://github.com/zweitag/zwen-react",
  "keywords": [
    "zweitag",
    "react",
    "redux",
    "generator",
    "cli",
    "architecture"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "zwen"
  },
  "repository": "https://github.com/zweitag/zwen-react",
  "scripts": {
    "build": "rm -rf lib && tsc && cp -R src/generators/templates lib/generators/templates/",
    "prepare": "npm test && rm -rf lib && tsc && cp -R src/generators/templates lib/generators/templates/",
    "test": "node_modules/.bin/mocha --require ts-node/register \"src/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
