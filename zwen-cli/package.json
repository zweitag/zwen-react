{
  "name": "zwen",
  "description": "zwen is a code generator CLI for projects using react and redux",
  "version": "0.0.1",
  "author": "Zweitag GmbH @zweitag",
  "bin": {
    "zwen": "./bin/run"
  },
  "bugs": "https://github.com/https://github.com/zweitag/zwen-react/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/plugin-help": "1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/test": "1",
    "@oclif/tslint": "1",
    "@types/chai": "4",
    "@types/mocha": "5",
    "@types/node": "9",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "nyc": "11",
    "ts-node": "5",
    "tslib": "1",
    "tslint": "5",
    "typescript": "2.8",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/https://github.com/zweitag/zwen-react",
  "keywords": [
    "oclif",
    "zweitag",
    "react",
    "redux",
    "generator",
    "cli",
    "architecture"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "zwen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/zweitag/zwen-react",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test_mocha": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "test": "npm test",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "types": "lib/index.d.ts"
}
